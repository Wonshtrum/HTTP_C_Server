<!DOCTYPE html>
<?mrm
v1 = int(MRM.get("v1", 0))
v2 = int(MRM.get("v2", 0))
op = MRM.get("op")
if op == "add":
	print(v1, "+", v2, "=", v1+v2)
elif op == "sub":
	print(v1, "-", v2, "=", v1-v2)
elif op == "mul":
	print(v1, "x", v2, "=", v1*v2)
elif op == "div":
	print(v1, "/", v2, "=", v1/v2)
	#if v2 is 0, then Marimba will fail rendering this page
else:
	print("incorect operator")
?>
